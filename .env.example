# Environment configuration for Face Blur Project
# Copy this file to .env and modify as needed

# Python version (for reference)
PYTHON_VERSION=3.9

# Application directories
WHITELIST_DIR=whitelist
OUTPUT_DIR=output

# DeepFace configuration
DEEPFACE_DETECTOR_BACKEND=opencv
DEEPFACE_MODEL_NAME=VGG-Face

# Video processing settings
DEFAULT_BLUR_KERNEL_SIZE=99
DEFAULT_BLUR_SIGMA=30
FACE_MATCHING_THRESHOLD=0.5

# UI settings
DEFAULT_WINDOW_TITLE=Face Blur App
DEFAULT_BUTTON_WIDTH=30